{
  "name": "loan-application-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "==next==": "------------------------------------------------------------------------------------------",
    "dev": "env-cmd -f ./.config/dev.env node server.js",
    "dev:nodemon": " nodemon --exec \"env-cmd -f ./.config/dev.env node --inspect server.js\"  -w ./server.js -w ./next.config.js -w ./.config/dev.env",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production node server.js",
    "test": "jest",
    "==lint&format==": "-----------------------------------------------------------------------------------",
    "format": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "lint": "jest --config test/jest.lint.js",
    "==docker&prisma==": "---------------------------------------------------------------------------------",
    "up": "docker-compose up -d",
    "down": "docker-compose down --remove-orphans",
    "deploy": "prisma deploy",
    "schema": "prisma generate",
    "token": "prisma token",
    "==prisma-dev==": "--------------------------------------------------------------------------------------",
    "dev:ready": "concurrently \"npm run dev:up\" \"npm run dev:browse\"",
    "dev:browse": "cross-env-shell \"sleep 3\" && node ./terminalString/mdev.js && cross-env-shell \"sleep 5\" && env-cmd -f ./.config/dev.env npm run playground",
    "dev-nodemon": "env-cmd -f ./.config/dev.env nodemon src/index.js --ext js,graphql --exec babel-node",
    "dev:deploy": "env-cmd -f ./.config/dev.env npm run deploy && npm run dev:schema",
    "dev:schema": "env-cmd -f ./.config/dev.env npm run schema",
    "dev:token": "env-cmd -f ./.config/dev.env npm run token",
    "dev:up": "env-cmd -f ./.config/dev.env npm run up",
    "dev:down": "env-cmd -f ./.config/dev.env npm run down",
    "==prisma:staging==": "------------------------------------------------------------------------------------",
    "staging:deploy": "env-cmd -f ./.config/staging.env npm run deploy && npm run staging:schema",
    "staging:schema": "env-cmd -f ./.config/staging.env npm run schema",
    "staging:token": "env-cmd -f ./.config/staging.env npm run token",
    "staging:up": "env-cmd -f ./.config/staging.env npm run up",
    "staging:down": "env-cmd -f ./.config/staging.env npm run down"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "fix": true,
      "ext": [
        ".js",
        ".ts"
      ]
    }
  },
  "dependencies": {
    "@sentry/browser": "^5.10.2",
    "@sentry/node": "5.10.2",
    "@types/currency-formatter": "^1.3.0",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "connect-redis": "^4.0.3",
    "currency-formatter": "^1.5.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-sslify": "^1.2.0",
    "formidable": "^1.2.1",
    "formik": "^2.0.8",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "hellosign-sdk": "^1.3.1",
    "libphonenumber-js": "^1.7.29",
    "mailgun.js": "^2.0.1",
    "mangopay2-nodejs-sdk": "^1.10.0",
    "moment": "^2.29.4",
    "next": "9.1.5",
    "next-compose-plugins": "^2.2.0",
    "next-cookies": "^2.0.3",
    "next-fonts": "^0.19.0",
    "next-images": "^1.2.0",
    "node-cron": "^2.0.3",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.1",
    "prisma": "^1.34.10",
    "prisma-client-lib": "^1.34.10",
    "ramda": "^0.26.1",
    "react": "16.12.0",
    "react-autosize-textarea": "^7.0.0",
    "react-dom": "16.12.0",
    "react-error-boundary": "^1.2.5",
    "react-toastify": "^5.4.1",
    "redis": "^2.8.0",
    "styled-components": "^4.3.2",
    "tailwindcss": "^1.1.4",
    "tmp-promise": "^2.0.2",
    "uk-modulus-checking": "^0.1.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "node-mocks-http": "^1.8.1",
    "test-listen": "^1.1.0",
    "@types/formidable": "^1.0.31",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.2",
    "cors": "^2.8.5",
    "env-cmd": "^10.0.1",
    "eslint": "^6.7.2",
    "eslint-config-esnext": "^4.0.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.7.5",
    "jest-watch-select-projects": "^1.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.2",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "shell-quote": "^1.7.2",
    "tachyons": "^4.11.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  }
}
